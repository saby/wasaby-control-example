<div attr:class="controls-Diagram
    {{_options.borderVisible ? ' controls-Diagram_bordered' : '' }}">
    <div attr:class="controls-Diagram__wrapper">
        <ws:for data="index, item in _viewData">
            <div attr:class="controls-Diagram__category controls-Diagram__category__{{index}}"
                 attr:style="width:{{item}}%" attr:title="{{item}}%"></div>
        </ws:for>
    </div>
</div>